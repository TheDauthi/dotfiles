#!/usr/bin/env bash

set -e

source '.installer/utilities.lib.sh'
source '.installer/dotbot.lib.sh'
source '.installer/python.lib.sh'

#CONFIG="install.conf.yaml"

DOTBOT_DIR=".installer/dotbot"
DOTBOT_BIN="${DOTBOT_DIR}/bin/dotbot"

BASEDIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"

cd "${BASEDIR}"

_install_via_dotbot $BASEDIR 